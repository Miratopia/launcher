{
  "name": "launcher_test_rust",
  "private": true,
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:no-fork": "nuxt dev --no-fork",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "tauri": "tauri",
    "tauri:no-browser": "BROWSER=none tauri",
    "tauri:signer:generate": "bunx tauri signer generate --force -w .tauri/miratopia-launcher.key",
    "install-sccache": "bunx cargo install sccache --locked || true",
    "tauri:build-fast": "cross-env RUSTC_WRAPPER=sccache bunx tauri build",
    "tauri:dev-fast": "cross-env RUSTC_WRAPPER=sccache bunx tauri dev",
    "tauri:build:win": "$private = Get-Content -Raw '.\\.tauri\\miratopia-launcher.key'; $env:TAURI_SIGNING_PRIVATE_KEY = $private; bun run tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.10.0",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-store": "~2",
    "@tauri-apps/plugin-stronghold": "^2.3.1",
    "@tauri-apps/plugin-updater": "~2",
    "@tauri-apps/plugin-window-state": "^2.4.1",
    "lucide-vue-next": "^0.575.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@pinia/nuxt": "0.11.3",
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^25.3.0",
    "@types/tailwindcss": "^3.1.0",
    "cross-env": "^7.0.3",
    "nuxt": "^4.2.2",
    "typescript": "~5.6.2"
  }
}
